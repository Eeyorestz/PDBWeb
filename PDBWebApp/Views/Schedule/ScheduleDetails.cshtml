
@model PDBWebApp.ViewModels.SchedulesViewModel

<table>
    @foreach (var schedule in Model.Schedules)
    {
        <tr>
            <td><a asp-controller="FillInfo" asp-action="FillForm" asp-route-id3="@schedule.Id">@schedule.ScheduleValue</a></td>
        </tr>
    }

</table>

<a asp-action="Index" asp-controller="Home">Back to home</a>
